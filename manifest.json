{
  "name": "Dumpert Video Fixer",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "*://www.dumpert.nl/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}